{
  "name": "beekeeper-util",
  "version": "11.2.0",
  "description": "CLI utility for managing the beekeeper-service",
  "keywords": [
    "beekeeper"
  ],
  "homepage": "https://github.com/octoblu/beekeeper-util",
  "bugs": "https://github.com/octoblu/beekeeper-util/issues",
